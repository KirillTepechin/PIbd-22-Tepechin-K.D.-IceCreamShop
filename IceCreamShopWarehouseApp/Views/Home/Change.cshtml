@{
    ViewData["Title"] = "Change";
}
<div class="text-center">
    <h2 class="display-4">Изменить склад склада</h2>
</div>
<form method="post">
    <div class="row">
        <div class="col-4">Склад:</div>
        <div class="col-8">
            <select id="iceCream" name="warehouse" class="form-control" asp-items="@(new SelectList(@ViewBag.Warehouses," Id", "WarehouseName" ))"></select>
        </div>
    </div>
    <div class="row">
        <div class="col-4">Название склада:</div>
        <div class="col-8"><input type="text" name="warehouseName" id="warehouseName" /></div>
    </div>
    <div class="row">
        <div class="col-4">Ответственное лицо:</div>
        <div class="col-8">
            <input type="text" name="responsiblePerson" id="responsiblePerson" />
        </div>
    </div>
    <div class="row">
        <div class="col-8"></div>
        <div class="col-4">
            <input type="submit" value="Изменить" class="btn btn-primary" />
        </div>
    </div>
</form>
